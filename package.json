{
  "name": "boilerplate-full-stack-auth0",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack"
  ],
  "scripts": {
    "start": "node server",
    "test": "jest",
    "test:watch": "jest --noStackTrace --watchAll",
    "dev": "run-p dev:client dev:server tailwind",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon --watch server server/index.js",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "lint": "eslint --ext .js,.jsx .",
    "format": "prettier -w server client",
    "webpack": "webpack --config ./client/webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "seed": "knex --knexfile ./server/db/knexfile.js seed:run",
    "tailwind": "tailwindcss -i ./server/public/input.css -o ./server/public/output.css --watch "
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "engines": {
    "node": "14.16",
    "npm": "6.14.11"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "daisyui": "^2.31.0",
    "express": "^4.17.2",
    "express-jwt": "^6.1.0",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^2.0.4",
    "knex": "^1.0.3",
    "dotenv": "^10.0.0",
    "leaflet": "^1.9.1",
    "pg": "^8.7.1",
    "react-loading": "^2.0.3",
    "react-range": "^1.8.14",
    "react-slider": "^2.0.4",
    "superagent": "7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.1.3",
    "@vscode/sqlite3": "^5.0.7",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-config-eda": "^1.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.3.0",
    "moment": "^2.29.4",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "sass": "^1.50.1",
    "sass-loader": "12.6.0",
    "style-loader": "^3.3.0",
    "supertest": "^6.2.2",
    "tailwindcss": "^3.1.8",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
